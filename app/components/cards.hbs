<div class="cards">
  <div class="card">
    <PaperCard as |card|>
      <card.title as |title|>
        <title.text as |text|>
          <text.headline>Card with image</text.headline>
          <text.subhead>Large</text.subhead>
        </title.text>
        {{paper-icon "accessibility"}}
      </card.title>
      <card.actions>
        <PaperButton>Action 1</PaperButton>
        <PaperButton>Action 2</PaperButton>
      </card.actions>
    </PaperCard>
  </div>

  <div class="card">
